    <style name="UgiButton">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:minWidth">0dp</item>
        <item name="android:minHeight">0dp</item>
        <item name="android:layout_gravity">center</item>
        <item name="android:paddingLeft">10dp</item>
        <item name="android:paddingRight">10dp</item>
        <item name="android:background">@android:color/transparent</item>
        <item name="android:textColor">@color/ugi_ui</item>
        <item name="android:textSize">18sp</item>
        <item name="android:textAllCaps" ns1:targetApi="ice_cream_sandwich">false</item>
    </style>
    <style name="UgiCheckbox">
        <item name="android:tag">UgiCheckbox</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_gravity">center_vertical</item>
        <item name="android:textColor">#000000</item>
    </style>
    <style name="UgiEditText">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColor">#000000</item>
        <item name="android:inputType">text</item>
        <item name="android:padding">4dp</item>
    </style>
    <style name="UgiFooterView" ns1:keep="@style/UgiFooterView">
        <item name="android:id">@id/ugi_footer_view</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
    </style>
    <style name="UgiHorizontalSpacerView" ns1:keep="@style/UgiHorizontalSpacerView">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_weight">1</item>
        <item name="android:layout_height">1dp</item>
    </style>
    <style name="UgiRadioButton" ns1:keep="@style/UgiRadioButton">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textColor">#000000</item>
        <item name="android:buttonTint" ns1:targetApi="lollipop">@color/ugi_ui</item>
    </style>
    <style name="UgiSeekBar" ns1:keep="@style/UgiSeekBar">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_gravity">center</item>
        <item name="android:progressDrawable">@drawable/seekbar</item>
        <item name="android:thumbTint" ns1:ignore="NewApi">@color/ugi_ui</item>
    </style>
    <style name="UgiSegmentedControl" ns1:keep="@style/UgiSegmentedControl">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="textColorOnBackground">#000000</item>
    </style>
    <style name="UgiSpinner" ns1:keep="@style/UgiSpinner">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:background">@android:color/transparent</item>
        <item name="android:popupBackground">@drawable/ugi_spinner_background</item>
    </style>
    <style name="UgiTextView">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textSize">18sp</item>
        <item name="android:textColor">#000000</item>
        <item name="android:layout_gravity">center</item>
        <item name="android:gravity">center</item>
    </style>
    <style name="UgiTitleView" ns1:keep="@style/UgiTitleView">
        <item name="android:id">@id/ugi_title_view</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
    </style>
    <style name="ugi_dialog_OvershootDialogAnimation">
        <item name="android:windowEnterAnimation">@anim/ugi_dialog_bounce_enter</item>
        <item name="android:windowExitAnimation">@anim/ugi_dialog_quick_fade</item>
    </style>
